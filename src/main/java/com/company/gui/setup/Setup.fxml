<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.gui.setup.SetupPresenter">
   <children>
      <VBox layoutX="250.0" layoutY="14.0" prefHeight="200.0" prefWidth="1280.0" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="136.0" prefWidth="600.0" spacing="20.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="mappingInfo" prefHeight="37.0" prefWidth="209.0" text="Select the Mapping .csv" />
                        <Button fx:id="Select" mnemonicParsing="false" onAction="#onSelectMapping" prefHeight="39.0" prefWidth="52.0" text="Button" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" layoutX="10.0" layoutY="28.0" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="answersInfo" prefHeight="37.0" prefWidth="209.0" text="Select the Answers .csv" />
                        <Button fx:id="Select1" mnemonicParsing="false" onAction="#onSelectAnswers" prefHeight="39.0" prefWidth="52.0" text="Button" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="TOP_CENTER" prefHeight="63.0" prefWidth="600.0" spacing="50.0">
               <padding>
                  <Insets top="15.0" />
               </padding>
               <children>
                  <Button mnemonicParsing="false" onAction="#onStart" prefHeight="50.0" prefWidth="200.0" text="Start Rating Bot" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
